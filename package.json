{
  "name": "@onyx-and-iris/meld-cli",
  "version": "0.4.0",
  "description": "A command line interface for Meld Studio WebChannel API",
  "keywords": [
    "meld",
    "meld-studio",
    "meld-cli"
  ],
  "homepage": "https://github.com/onyx-and-iris/meld-api#readme",
  "bugs": {
    "url": "https://github.com/onyx-and-iris/meld-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onyx-and-iris/meld-cli.git"
  },
  "license": "MIT",
  "author": "onyx-and-iris",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "bin": {
    "meld-cli": "index.js"
  },
  "dependencies": {
    "cli-color": "^2.0.4",
    "cli-meow-help": "^4.0.0",
    "cli-table3": "^0.6.5",
    "meow": "^13.2.0",
    "qwebchannel": "^6.2.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "standard": "^17.1.2"
  },
  "standard": {
    "ignore": [
      "node_modules",
      "dist",
      "coverage",
      "test",
      "tests",
      "examples",
      "example",
      "docs",
      "doc"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    }
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "rules": {
      "no-unused-vars": "off",
      "no-console": "off"
    }
  }
}
